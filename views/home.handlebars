<section class="container my-4">
 <article>

 <div class="card-columns">
 {{#each payload}}
 <div class="card">
 <img
 class="card-img-top"
  src="{{trimString emp_photo.0.path}}" alt="{{emp_name}}"  >
              {{!-- //trimString is helper handlebar  --}}
    


 <div class="card-body">
 <h5 class="card-title text-capitalize">{{emp_name}}</h5>
 <p class="card-text">{{emp_email}}</p>
 <p class="card-text"><small class="text-muted">Last updated an hour ago
 ago</small></p>
 <p>
 <a href="/employee/{{_id}}" class="btn btn-primary btn-sn btn-block">view profile</a>
  <a href="/employee/edit-Emp/{{_id}}" class="btn btn-primary btn-sn btn-block ">edit profile</a>
 </p>
 </div>
 </div>
 {{/each}}
 </div>

 </article>
</section>  


 

 